<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="welcomeLoginVM" type="com.amirsohelsapplication.app.modules.welcomelogin.data.viewmodel.WelcomeLoginVM"/>
    </data>
    <FrameLayout
        android:id="@+id/frameWelcomeLogin"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        style="@style/groupStylewhite_A700">

        <FrameLayout android:id="@+id/frameStackor"
            android:layout_gravity="bottom|center"
            android:layout_height="@dimen/_352pxv" android:layout_width="match_parent">
            <ImageView android:id="@+id/imageImage"
                android:layout_gravity="center"
                android:layout_height="@dimen/_352pxh"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_rectangle}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle"/>
            <LinearLayout android:id="@+id/linearColumnor"
                android:layout_gravity="bottom|center"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_36pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnOr"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_34pxh"
                    android:layout_width="@dimen/_37pxh"
                    android:paddingBottom="@dimen/_9pxv"
                    android:paddingEnd="@dimen/_10pxh"
                    android:paddingStart="@dimen/_10pxh"
                    android:paddingTop="@dimen/_9pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_or"
                    android:textAllCaps="false" style="@style/btn" tools:text="@string/lbl_or"/>
                <View android:background="@color/gray_400"
                    android:id="@+id/lineLinetwoOne"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_13pxv" android:layout_width="@dimen/_360pxh"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/btnLoginWith"
                    android:layout_width="@dimen/_194pxh"
                    android:layout_height="@dimen/_51pxv"
                    android:layout_marginTop="@dimen/_50pxv"
                    android:layout_gravity="center_horizontal"
                    android:drawablePadding="@dimen/_13pxh"
                    android:ellipsize="end"
                    android:gravity="center"
                    android:paddingStart="@dimen/_27pxh"
                    android:paddingEnd="@dimen/_27pxh"
                    android:paddingTop="@dimen/_13pxv"
                    android:paddingBottom="@dimen/_13pxv"
                    android:singleLine="true"
                    android:text="@string/msg_login_with_google"
                    android:textAllCaps="false"
                    style="@style/btnSolidRoundedOutline"
                    tools:text="@string/msg_login_with_google"
                    android:drawableStart="@drawable/goog" />
                <LinearLayout
                    android:layout_gravity="center_horizontal"
                    android:gravity="center_horizontal"
                    android:id="@+id/linearRowdonthave"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_28pxv"
                    android:layout_width="wrap_content"
                    android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:gravity="top"
                        android:id="@+id/txtDonthave"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{welcomeLoginVM.welcomeLoginModel.txtDonthave}"
                        style="@style/txtRalewayromanmedium14" tools:text="@string/msg_don_t_have_account"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtRegister"
                        android:layout_gravity="bottom"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_5pxh"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{welcomeLoginVM.welcomeLoginModel.txtRegister}"
                        style="@style/txtRalewayromansemibold14"
                        textUnderline="true" tools:text="@string/lbl_register"/>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtTermAndConditio"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_33pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{welcomeLoginVM.welcomeLoginModel.txtTermAndConditio}"
                    style="@style/txtRalewayromanregular8" tools:text="@string/msg_by_continuing_you"/>
            </LinearLayout>
        </FrameLayout>


        <FrameLayout android:id="@+id/frameStackhomelogo"
            android:layout_gravity="top|center"
            android:layout_height="@dimen/_491pxv"
            android:layout_width="match_parent">

            <ImageView
                android:id="@+id/imageImageOne"
                android:layout_gravity="center"
                android:layout_height="@dimen/_491pxh"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                tools:ignore="ContentDescription"/>

            <LinearLayout
                android:id="@+id/linearColumnhomelogo"
                android:layout_gravity="bottom|center"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_17pxv"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_width="match_parent"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/imageHomelogoOne"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_94pxh"
                    android:layout_width="@dimen/_210pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_homelogo}"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_homelogo"/>

                <LinearLayout android:id="@+id/linearColumnstartyour"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_162pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtStartyourcurevi"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{welcomeLoginVM.welcomeLoginModel.txtStartyourcurevi}"
                        style="@style/txtRadleyregular12" tools:text="@string/msg_start_your_curevive"/>
                    <LinearLayout android:gravity="center_vertical"
                        android:id="@+id/linearRowenteryour"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_width="match_parent"
                        android:orientation="horizontal"
                        android:paddingHorizontal="@dimen/_5pxh"
                        android:paddingVertical="@dimen/_5pxv" style="@style/groupStylewhite_A700cornerRadius">
                        <LinearLayout android:gravity="center_vertical"
                            android:id="@+id/linearRowcloseOne"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_2pxv"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_width="wrap_content" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageCloseOne"
                                android:layout_gravity="bottom"
                                android:layout_height="@dimen/_14pxh"
                                android:layout_marginBottom="@dimen/_6pxv"
                                android:layout_marginTop="@dimen/_8pxv"
                                android:layout_width="@dimen/_14pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_close"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_close"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtNinetyone"
                                android:layout_gravity="center_vertical"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_6pxv"
                                android:layout_marginStart="@dimen/_3pxh"
                                android:layout_marginTop="@dimen/_7pxv"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="@{welcomeLoginVM.welcomeLoginModel.txtNinetyone}"
                                style="@style/txtRobotoromanregular12" tools:text="@string/lbl_91"/>
                            <View android:background="@color/gray_600"
                                android:id="@+id/lineLineoneOne"
                                android:layout_gravity="center_vertical"
                                android:layout_height="@dimen/_28pxv"
                                android:layout_marginStart="@dimen/_16pxh" android:layout_width="@dimen/_1pxh"/>
                        </LinearLayout>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtEnteryour"
                            android:layout_gravity="center_vertical"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_9pxv"
                            android:layout_marginStart="@dimen/_11pxh"
                            android:layout_marginTop="@dimen/_9pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{welcomeLoginVM.welcomeLoginModel.txtEnteryour}"
                            style="@style/txtRalewayromanregular12" tools:text="@string/msg_enter_your_number"/>
                        <ImageButton
                            android:background="@drawable/rectangle_bg_blue_50_radius_10"
                            android:id="@+id/btnArrowleftOne"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_34pxh"
                            android:layout_marginStart="@dimen/_101pxh"
                            android:layout_width="@dimen/_34pxh"
                            android:paddingHorizontal="@dimen/_10pxh"
                            android:paddingVertical="@dimen/_10pxv"
                            android:scaleType="fitCenter"
                            android:src="@drawable/img_arrow_left" tools:ignore="ContentDescription"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
        <TextView android:ellipsize="end"
            android:gravity="center"
            android:id="@+id/txtOtpwillbe"
            android:layout_gravity="bottom|center"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_297pxv"
            android:layout_width="match_parent"
            android:singleLine="true"
            android:text="@{welcomeLoginVM.welcomeLoginModel.txtOtpwillbe}"
            style="@style/txtJostromanmedium10"
            tools:text="@string/msg_otp_will_be_sent"/>
    </FrameLayout>
</layout>
