<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="registrationFormThreeVM" type="com.amirsohelsapplication.app.modules.registrationform3.data.viewmodel.RegistrationFormThreeVM"/>
    </data>
    <FrameLayout
        android:id="@+id/frameRegistrationForm"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        style="@style/groupStylewhite_A700">
        <ImageView android:id="@+id/imageImage"
            android:layout_gravity="center"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:scaleType="fitXY"
            imageRes="@{@drawable/img_rectangle}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle"/>
        <LinearLayout android:id="@+id/linearColumnwheredoyo"
            android:layout_gravity="center"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_60pxv"
            android:paddingEnd="@dimen/_19pxh"
            android:paddingStart="@dimen/_19pxh"
            android:paddingTop="@dimen/_60pxv">
            <ProgressBar
                android:gravity="center_horizontal"
                android:id="@+id/progressBarProgressbar"
                android:indeterminate="false"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_width="match_parent"
                android:max="100"
                android:minHeight="@dimen/_11pxv"
                android:progressDrawable="@drawable/layer_list_bg_dark_for_progressbar"
                style="@style/Widget.AppCompat.ProgressBar.Horizontal"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtThirtyThree"
                android:layout_gravity="end"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@string/lbl_3_3"
                style="@style/txtDMSerifDisplayregular14_2"
                tools:text="@string/lbl_3_3"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtWheredoyou"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_26pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{registrationFormThreeVM.registrationFormThreeModel.txtWheredoyou}"
                style="@style/txtDMSerifDisplayregular20"
                tools:text="@string/msg_where_do_you_stay"/>


            <ScrollView
                android:fillViewport="true"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/load"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

            <com.google.android.material.textfield.TextInputLayout
                android:hint="@string/lbl_pincode"
                android:id="@+id/textInputLayPincode1"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_11pxv"
                android:paddingEnd="@dimen/_28pxh"
                android:paddingStart="@dimen/_28pxh"
                android:paddingTop="@dimen/_11pxv"
                android:textColorHint="@color/gray_700"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox">

                <androidx.appcompat.widget.AppCompatSpinner
                android:background="@drawable/layer_list_border_gray_600_radius_10"
                android:gravity="center_horizontal"
                android:id="@+id/spinnerCountry"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_46pxv"
                android:layout_width="match_parent"
                tools:listitem="@layout/spinner_item"/>

            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:hint="@string/lbl_pincode"
                android:id="@+id/textInputLayPincode11"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_11pxv"
                android:paddingEnd="@dimen/_28pxh"
                android:paddingStart="@dimen/_28pxh"
                android:paddingTop="@dimen/_11pxv"
                android:textColorHint="@color/gray_700"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox">

            <androidx.appcompat.widget.AppCompatSpinner
                android:background="@drawable/layer_list_border_gray_600_radius_10"
                android:gravity="center_horizontal"
                android:id="@+id/spinnerState"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="match_parent"
                tools:listitem="@layout/spinner_item"/>

            </com.google.android.material.textfield.TextInputLayout>


            <com.google.android.material.textfield.TextInputLayout
                android:hint="@string/lbl_pincode"
                android:id="@+id/textInputLayPincode11111"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_11pxv"
                android:paddingEnd="@dimen/_28pxh"
                android:paddingStart="@dimen/_28pxh"
                android:paddingTop="@dimen/_11pxv"
                android:textColorHint="@color/gray_700"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox">
            <androidx.appcompat.widget.AppCompatSpinner
                android:background="@drawable/layer_list_border_gray_600_radius_10"
                android:gravity="center_horizontal"
                android:id="@+id/spinnerCity"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="match_parent"
                tools:listitem="@layout/spinner_item"/>

            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:hint="@string/lbl_pincode"
                android:id="@+id/textInputLayPincode"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_11pxv"
                android:paddingEnd="@dimen/_28pxh"
                android:paddingStart="@dimen/_28pxh"
                android:paddingTop="@dimen/_11pxv"
                android:textColorHint="@color/gray_700"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox">
                <com.google.android.material.textfield.TextInputEditText
                    android:ellipsize="end"
                    android:fontFamily="@font/jost"
                    android:id="@+id/etPincode" android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@={registrationFormThreeVM.registrationFormThreeModel.etPincodeValue}"
                    android:textColor="@color/gray_500"
                    android:textFontWeight="400"
                    android:textSize="@dimen/_20pxh"
                    android:textStyle="normal"
                    tools:text="@string/lbl_00000"/>
            </com.google.android.material.textfield.TextInputLayout>

            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnComplete"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:layout_marginTop="50dp"
                android:paddingBottom="@dimen/_21pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_21pxv"
                android:singleLine="true"
                android:text="@string/lbl_complete"
                android:textAllCaps="false"
                style="@style/btnSolidRounded"
                tools:text="@string/lbl_complete"/>

            </LinearLayout>
            </ScrollView>
        </LinearLayout>
    </FrameLayout>
</layout>
